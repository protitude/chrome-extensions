{
   "content_scripts": [
      {
        "css": ["css/shout.css"],
        "matches": ["https://www.colorado.edu/oit/*", "http://www.colorado.edu/oit/*"]
      }
    ],
   "browser_action": {
     "default_icon": {
        "48": "images/icon48.png"
     },
     "default_title": "Shout Menu"
   },
   "description": "Show and copy the url-shortlink",
   "icons": {
      "128": "images/icon128.png",
      "16": "images/icon48.png",
      "48": "images/icon128.png"
   },
   "manifest_version": 2,
   "name": "OIT Shout Menu",
   "permissions": [ "tabs", "\u003Call_urls>" ],
   "version": "1.0"
}
